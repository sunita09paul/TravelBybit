package com.demo;

public class Animal {
	
	int a = 20;
	public static void main(String[]arg) {
		 
		
		Dog d = new Dog();
	}
	
	
	  Animal(){ System.out.println("Animal is parent class"); }
	 

}
class Dog extends Animal{
	

	Dog(){
		int a =120;
		System.out.println("Dog is child class" +super.a);
	
	
	}

	 
		 
		 
	 }
	 
	 
	 

	
