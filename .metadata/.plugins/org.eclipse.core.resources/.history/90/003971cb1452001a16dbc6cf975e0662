package com.demo;

public class Animal {
	
	public static void main(String[]arg) {
		 
		 Dog d = new Dog();
		 
	}
	
	
	  Animal(){ System.out.println("Animal is parent class"); }
	 

}
class Dog extends Animal{
	
	
	Dog(){
		System.out.println("Dog is child class");
		
	}
	 
		 
		 
	 }
	 
	 
	 

	
