package com.demo;

public class Animal {
	
	 static int a = 20;
	 int  b = 50;
	public static void main(String[]arg) {
		 
		
		
	}
	
	public void methodOne() {
		Dog d = new Dog(); 
		d.i
	}
	
	  Animal(){ System.out.println("Animal is parent class"); }
	 

}
final class Dog extends Animal{
	

	Dog(){
		int a =120;
		System.out.println("Dog is child class" +a);
	
	
	}

		 
		 
